{% assign self = "liquid_data.xml" %}{% if include.obj.root %}<?xml version="1.0"?>
{% endif %}
<{% if include.obj.namespace %}{{ include.obj.namespace }}:{% endif %}{{ include.obj.name }}
{% for ns in include.obj.xmlns %}xmlns{% if ns.prefix %}{{ ns.prefix }}:{% endif %}="{{ ns.uri }}"{% endfor %}
{% for attribute in include.obj.attributes %}{% if attribute.namespace %}{{ attribute.namespace }}:{% endif %}{{ attribute.name }}="{ attribute.value }}"{% endfor %}>
{% for child in include.obj.children %}{% include {{ self }} obj=child %}{% endfor %}
</{% if include.obj.namespace %}{{ include.obj.namespace }}:{% endif %}{{ include.obj.name }}>

